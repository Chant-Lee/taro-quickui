{
  "package": "com.application.demo",
  "name": "taro-quickui",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.prompt"},
    { "name":  "@system.audio"},
    { "name":  "@system.media"}
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "home",
    "pages": {
      "home": {
        "component": "index"
      },
      "components/container/view": {
        "component": "index"
      },
      "components/container/swiper": {
        "component": "index"
      },
      "components/basic/icon": {
        "component": "index"
      },
      "components/basic/progress": {
        "component": "index"
      },
      "components/basic/rich_text": {
        "component": "index"
      },
      "components/basic/text": {
        "component": "index"
      },
      "components/basic/image": {
        "component": "index"
      },
      "components/form/input": {
        "component": "index"
      },
      "components/form/button": {
        "component": "index"
      },
      "components/form/checkbox": {
        "component": "index"
      },
      "components/form/slider": {
        "component": "index"
      },
      "components/form/switch": {
        "component": "index"
      },
      "components/form/textarea": {
        "component": "index"
      },
      "components/form/label": {
        "component": "index"
      },
      "components/form/radio": {
        "component": "index"
      },
      "components/form/picker": {
        "component": "index"
      },
      "components/media/video": {
        "component": "index"
      },
      "components/media/audio": {
        "component": "index"
      },
      "components/media/camera": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "home": {
        "titleBarText": "taro-quickui",
        "menu": false
      }
    }
  }
}