    "babel-runtime": "^6.9.2",
    "babel-template": "^6.24.1",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.0",
    "babylon-jsx": "^1.0.0",
    "chalk": "^1.1.3",
    "crypto-js": "^3.1.9-1",
    "css": "~2.2.1",
