  "subversion": {
    "packager": "0.0.5"
  },
  "description": "a webpack loader for hybrid app",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist/* && rm -rf lib/* && rm -rf test/build/*",
    "build": "npm run lint && babel src --out-dir lib",
